@model List<IndustriasTecGaem.BL.Producto>

@{
    ViewBag.Title = "Index";
}

<h2>Categoria</h2>


<table class="table table-bordered table-hover">
    <thead>
        <tr>
            <th>
                Codigo
            </th>
            <th>
                Descripcion
            </th>
        </tr>
    </thead>
    @foreach (var categorias in Model)
    {


        <tr>
            <td>@categorias.Id</td>
            <td>@categorias.Descripcion</td>
            
        </tr>
    }
</table>
}